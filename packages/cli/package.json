{
  "name": "@spurprotocol/cli",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "bin": {
    "spur": "dist/index.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "node --eval \"console.log('No tests for @spurprotocol/cli')\""
  },
  "dependencies": {
    "@spurprotocol/doctor": "workspace:*",
    "@spurprotocol/explain": "workspace:*",
    "@spurprotocol/types": "workspace:*"
  }
}
